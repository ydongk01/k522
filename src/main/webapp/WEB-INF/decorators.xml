<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorator">
	<decorator name="main" page="hello_decorator.jsp">
		<pattern>/hello/*</pattern>
	</decorator>
	<decorator name="news" page="news_decorator.jsp">
		<pattern>/news/*</pattern>
	</decorator>
	<excludes>
		<pattern>/hello/execlude.jsp</pattern>
		<pattern>/news/execlude.jsp</pattern>
	</excludes>
</decorators>